package dailydairy;

import java.util.Scanner;

public class SumOfAllDigitsInANumber {
	public static void main(String []args)
	{
	  Scanner sc=new Scanner(System.in);
      int n=sc.nextInt();
      sumOfDigits(n);	
    }
    static void sumOfDigits(int k)
    {
    	int r,s=0;
    	for( ;k>0; ) 
    	{
    	    r=k%10;
    	    s=s+r;
    	    k=k/10;
    	}
    	System.out.print(s);

    }
}
