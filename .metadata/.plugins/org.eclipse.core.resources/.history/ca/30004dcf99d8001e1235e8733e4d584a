package dailydairy;

import java.util.Scanner;

public class MultiplesOfaGivenNum {
	public static void main(String []args)
	{
	  Scanner sc=new Scanner(System.in);
      int n=sc.nextInt();
      int m=sc.nextInt();
      int v=multiplesOfaGivenNUm(n,m);
      System.out.print(v);
	}
	static int multiplesOfaGivenNUm(int k,int l)
	{ 
		//int j=(k<l)?k:l;
        for(int i=1; i<=k;i++)  
        {
        	if(i%k==0)
        	{
        		return i; 		
        	}	
        }
        return 0;

}
}
